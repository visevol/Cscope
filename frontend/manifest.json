{
    "version": "0.0.1",
    "manifest_version": 3,
    "name": "CScope",
    "description": "CScope",
    "permissions": [
        "declarativeContent",
        "scripting",
        "activeTab"
    ],
    "background": {
        "service_worker": "js/background.js"
    },
    "content_scripts": [
        {
            "js": [
                "js/content.js",
                "js/index.js"
            ],
            "matches": [
                "https://github.com/**"
            ]
        }
    ]
}